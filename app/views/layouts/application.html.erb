<!DOCTYPE html>
<html>
<head>
  <title>PunchStarter</title>
	<%= include_gon %>
	<%= stylesheet_link_tag "application", :media => "all" %>
  <script src="http://api.filepicker.io/v1/filepicker.js"></script>
	
  <script>filepicker.setKey(gon.filepicker_api);</script>
	
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div id="wrapper">
		<div id="header">
			<%= render "layouts/navbar" %>
		</div>

		<div id="content">
			<%= yield %>
		</div>

		<div id="footer">
			<%= render "layouts/footer" %>
		</div>
	</div>
</body>
</html>